{
  "rules": {
    ".read": false,
    ".write": false,
    
    "users": {
      ".read": true,
      ".write": "auth != null && root.child('user_roles').child(auth.uid).child('role').val() === 'admin'",
      "$uid": {
        ".read": true,
        ".write": "auth != null && root.child('user_roles').child(auth.uid).child('role').val() === 'admin'"
      }
    },
    
    "user_roles": {
      "$uid": {
        ".read": "auth != null && auth.uid === $uid",
        ".write": "auth != null && auth.uid === $uid"
      }
    },
    
    "resources": {
      ".read": true,
      ".write": "auth != null && root.child('user_roles').child(auth.uid).child('role').val() === 'admin'",
      "cours": {
        ".read": true,
        ".write": "auth != null && root.child('user_roles').child(auth.uid).child('role').val() === 'admin'",
        "$year": {
          ".read": true,
          ".write": "auth != null && root.child('user_roles').child(auth.uid).child('role').val() === 'admin'",
          "$module": {
            ".read": true,
            ".write": "auth != null && root.child('user_roles').child(auth.uid).child('role').val() === 'admin'",
            "$resourceId": {
              ".read": true,
              ".write": "auth != null && root.child('user_roles').child(auth.uid).child('role').val() === 'admin'"
            }
          }
        }
      },
      "td": {
        ".read": true,
        ".write": "auth != null && root.child('user_roles').child(auth.uid).child('role').val() === 'admin'",
        "$year": {
          ".read": true,
          ".write": "auth != null && root.child('user_roles').child(auth.uid).child('role').val() === 'admin'",
          "$module": {
            ".read": true,
            ".write": "auth != null && root.child('user_roles').child(auth.uid).child('role').val() === 'admin'",
            "$resourceId": {
              ".read": true,
              ".write": "auth != null && root.child('user_roles').child(auth.uid).child('role').val() === 'admin'"
            }
          }
        }
      },
      "seminar": {
        ".read": true,
        ".write": "auth != null && root.child('user_roles').child(auth.uid).child('role').val() === 'admin'",
        "$seminarId": {
          ".read": true,
          ".write": "auth != null && root.child('user_roles').child(auth.uid).child('role').val() === 'admin'"
        }
      }
    },
    
    "files": {
      ".read": true,
      ".write": "auth != null && root.child('user_roles').child(auth.uid).child('role').val() === 'admin'",
      "$type": {
        ".read": true,
        ".write": "auth != null && root.child('user_roles').child(auth.uid).child('role').val() === 'admin'",
        "$fileId": {
          ".read": true,
          ".write": "auth != null && root.child('user_roles').child(auth.uid).child('role').val() === 'admin'"
        }
      }
    }
  }
}
